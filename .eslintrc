{
  "env": {
   	"node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "import/no-named-as-default": 0,
    "linebreak-style": 0,
    "consistent-return": 0,
    "max-len": 0
  }
}